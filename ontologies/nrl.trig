    <http://semanticdesktop.org/ontologies/2007/08/15/nrl>
          a       nrl:DocumentGraph , nrl:KnowledgeBase ;
          nao:hasNamespace "http://semanticdesktop.org/ontologies/2007/08/15/nrl#" ;
          nao:hasNamespaceAbbreviation "nrl" ;
          nao:lastModified "2007-08-22T13:38:32.866Z" ;

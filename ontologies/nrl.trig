    <http://semanticdesktop.org/ontologies/2007/08/15/nrl/metadata>
          a       nrl:GraphMetadata , nrl:DocumentGraph ;
          nao:serializationLanguage "TriG" ;
          nrl:coreGraphMetadataFor <http://semanticdesktop.org/ontologies/2007/08/15/nrl> .

}
